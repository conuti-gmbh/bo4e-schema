{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Ansprechpartner.schema.json",
  "title": "Ansprechpartner",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "ANSPRECHPARTNER"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1",
      "description": "versionStruktur"
    },
    "nachname": {
      "type": "string",
      "faker": "person.lastName",
      "description": "nachname"
    },
    "eMailAdresse": {
      "type": "string",
      "faker": "internet.email",
      "description": "eMailAdresse"
    },
    "rufnummern": {
      "type": "array",
      "items": {
        "$ref": "../com/Rufnummer.schema.json"
      },
      "description": "rufnummern"
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
