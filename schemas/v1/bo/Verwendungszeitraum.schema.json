{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Verwendungszeitraum.schema.json",
  "title": "Verwendungszeitraum",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "VERWENDUNGSZEITRAUM"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1"
    },
    "verwendungAb": {
      "type": "string",
      "format": "date-time"
    },
    "verwendungBis": {
      "type": "string",
      "format": "date-time"
    },
    "zeitraumId": {
      "type": "integer",
      "faker": {
        "number.int": {
          "min": 1,
          "max": 9
        }
      }
    },
    "qualitaet": {
      "$ref": "../enum/Verwendungszeitraumqualitaet.schema.json"
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
