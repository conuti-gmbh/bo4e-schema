{
  "$schema": "http://json-schema.org/2020-12/schema#",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Vertrag.schema.json",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "VERTRAG"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1"
    },
    "sparte": {
      "$ref": "../enum/Sparte.schema.json"
    },
    "vertragsart": {
      "type": "string"
    },
    "vertragsnummer": {
      "type": "string"
    },
    "beschreibung": {
      "type": "string"
    },
    "lokationsId": {
      "type": "string",
      "faker": {
        "string.numeric": 11
      }
    },
    "lokationsTyp": {
      "$ref": "../enum/Lokationstyp.schema.json"
    },
    "vertragsstatus": {
      "$ref": "../enum/Vertragstatus.schema.json"
    },
    "vertragsende": {
      "type": "string",
      "format": "date-time"
    },
    "vertragsbeginn": {
      "type": "string",
      "format": "date-time"
    },
    "gemeinderabatt": {
      "type": "integer",
      "faker": {
        "number.int": {
          "min": 10,
          "max": 30
        }
      }
    },
    "vertragskonditionen": {
      "$ref": "../com/Vertragskonditionen.schema.json"
    },
    "korrespondenzpartner": {
      "$ref": "Geschaeftspartner.schema.json"
    },
    "abrechnungUeberNna": {
      "type": "boolean"
    },
    "vertragspartner1": {
      "type": "array",
      "items": {
        "$ref": "Geschaeftspartner.schema.json"
      }
    },
    "vertragspartner2": {
      "type": "array",
      "items": {
        "$ref": "Geschaeftspartner.schema.json"
      }
    },
    "enFG": {
      "type": "array",
      "items": {
        "$ref": "../com/EnFG.schema.json"
      }
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
