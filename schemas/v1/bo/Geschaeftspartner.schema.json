{
  "$schema": "http://json-schema.org/2020-12/schema#",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Geschaeftspartner.schema.json",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "GESCHAEFTSPARTNER"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1"
    },
    "anrede": {
      "type": "string",
      "faker": "person.prefix"
    },
    "name1": {
      "type": "string",
      "faker": "person.lastName"
    },
    "name2": {
      "type": "string",
      "faker": "person.firstName"
    },
    "name3": {
      "type": "string"
    },
    "name4": {
      "type": "string"
    },
    "umsatzsteuerId": {
      "type": "string",
      "faker": {
        "string.binary": {
          "prefix": "DE",
          "length": 9
        }
      }
    },
    "glaeubigerId": {
      "type": "string",
      "faker": {
        "string.numeric": 6
      }
    },
    "emailAdresse": {
      "type": "string",
      "faker": "internet.email"
    },
    "website": {
      "type": "string",
      "faker": "internet.url"
    },
    "gewerbekennzeichnung": {
      "type": "boolean"
    },
    "hrnummer": {
      "type": "string",
      "faker": {
        "string.numeric": 6
      }
    },
    "amtsgericht": {
      "type": "string",
      "faker": "location.city"
    },
    "partneradresse": {
      "$ref": "../com/Adresse.schema.json"
    },
    "externeKundenummerLieferant": {
      "type": "string",
      "faker": {
        "string.numeric": 6
      }
    },
    "externeReferenzen": {
      "type": "array",
      "items": {
        "$ref": "../com/ExterneReferenz.schema.json"
      }
    },
    "geschaeftspartnerrolle": {
      "type": "array",
      "items": {
        "$ref": "../enum/Geschaeftspartnerrolle.schema.json"
      }
    },
    "kontaktweg": {
      "type": "array",
      "items": {
        "$ref": "../enum/Kontaktart.schema.json"
      }
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
