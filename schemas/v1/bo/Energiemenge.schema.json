{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Energiemenge.schema.json",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "ENERGIEMENGE"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1"
    },
    "lokationsId": {
      "type": "string",
      "faker": {
        "string.numeric": 11
      }
    },
    "lokationsTyp": {
      "$ref": "../enum/Lokationstyp.schema.json"
    },
    "fertigstellungsdatum": {
      "type": "string",
      "format": "date-time"
    },
    "startdatum": {
      "type": "string",
      "format": "date-time"
    },
    "enddatum": {
      "type": "string",
      "format": "date-time"
    },
    "bilanzierungsdatum": {
      "type": "string",
      "format": "date-time"
    },
    "beginndatum": {
      "type": "string",
      "format": "date-time"
    },
    "referenzStammdatenmeldungMsb": {
      "type": "string",
      "faker": {
        "string.binary": {
          "prefix": "REF",
          "length": 6
        }
      }
    },
    "konfiguration": {
      "type": "string"
    },
    "energieverbrauch": {
      "type": "array",
      "items": {
        "$ref": "../com/Verbrauch.schema.json"
      }
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
