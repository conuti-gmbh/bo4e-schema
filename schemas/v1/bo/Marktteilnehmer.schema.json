{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/conuti-gmbh/bo4e-schema/master/schemas/v1/bo/Marktteilnehmer.schema.json",
  "type": "object",
  "properties": {
    "boTyp": {
      "$ref": "../enum/BOTyp.schema.json",
      "default": "MARKTTEILNEHMER"
    },
    "versionStruktur": {
      "type": "string",
      "default": "1"
    },
    "geschaeftspartnerrolle": {
      "$ref": "../enum/Geschaeftspartnerrolle.schema.json"
    },
    "anrede": {
      "type": "string",
      "faker": "person.prefix"
    },
    "name1": {
      "type": "string",
      "faker": "person.lastName"
    },
    "name2": {
      "type": "string",
      "faker": "person.firstName"
    },
    "name3": {
      "type": "string"
    },
    "name4": {
      "type": "string"
    },
    "partneradresse": {
      "$ref": "../com/Adresse.schema.json"
    },
    "gewerbekennzeichnung": {
      "type": "boolean"
    },
    "externeKundenummerLieferant": {
      "type": "string"
    },
    "marktrolle": {
      "$ref": "../enum/Marktrolle.schema.json"
    },
    "rollencodenummer": {
      "type": "string",
      "faker": {
        "string.numeric": 9
      }
    },
    "rollencodetyp": {
      "type": "string"
    },
    "umsatzsteuerId": {
      "type": "string",
      "faker": {
        "string.binary": {
          "prefix": "DE",
          "length": 9
        }
      }
    },
    "steuernummer": {
      "type": "string",
      "faker": {
        "string.numeric": 9
      }
    },
    "ansprechpartner": {
      "$ref": "Ansprechpartner.schema.json"
    },
    "makoadresse": {
      "type": "string"
    },
    "downloadlinkZertifikat": {
      "type": "string",
      "faker": "internet.url"
    },
    "amtsgericht": {
      "type": "string",
      "faker": "location.city"
    },
    "hrnummer": {
      "type": "string",
      "faker": {
        "string.numeric": 6
      }
    },
    "website": {
      "type": "string",
      "faker": "internet.url"
    },
    "faxnummer": {
      "type": "string",
      "faker": "phone.number"
    },
    "kommunikationsrolle": {
      "$ref": "../enum/Kommunikationsrolle.schema.json"
    },
    "weiterverpflichtet": {
      "type": "boolean"
    },
    "kommunikationsparameter": {
      "$ref": "../com/Kommunikationsparameter.schema.json"
    },
    "messstellenbetreiberEigenschaft": {
      "$ref": "../enum/MSBEigenschaft.schema.json"
    },
    "bankverbindung": {
      "type": "array",
      "items": {
        "$ref": "../com/Bankverbindung.schema.json"
      }
    },
    "erreichbarkeit": {
      "type": "array",
      "items": {
        "$ref": "../com/Erreichbarkeit.schema.json"
      }
    },
    "ipAdresse": {
      "type": "string",
      "faker": "internet.ip"
    },
    "ipRange": {
      "$ref": "../com/IpRange.schema.json"
    }
  },
  "required": [
    "boTyp",
    "versionStruktur"
  ]
}
